[core]
        editor = vim
[log]
        date = iso
[alias]
        st = status
        co = checkout
        br = branch
        up = rebase
        ci = commit
        lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
        rr = remote -v
        ac = !git add -A && git commit
        puff = pull --ff-only
[init]
        defaultBranch = master

